{
  "name": "doc-pre-data",
  "version": "1.3.2",
  "description": "整理openapi数据",
  "main": "lib/index.js",
  "module": "lib/esm/index.js",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/index.js"
    }
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "dev": "ts-node ./src/dev.ts",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "openapi": "ts-node ./src/openapi/index.ts",
    "release": "standard-version -- --first-release"
  },
  "keywords": [
    "openapi"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/space-77/openapiFormat"
  },
  "bugs": {
    "url": "https://github.com/space-77/openapiFormat/issues"
  },
  "homepage": "https://github.com/space-77/openapiFormat",
  "author": "space-77",
  "license": "MIT",
  "dependencies": {
    "axios": "*",
    "do-swagger2openapi": "^7.0.20",
    "is-chinese": "^2.0.0",
    "is-ecma-keyword": "^1.0.3",
    "jsonrepair": "^3.8.0",
    "lodash": "*",
    "node-translates": "^1.4.1",
    "openapi-types": "^12.0.2",
    "pinyin-pro": "^3.13.1",
    "traverse": "^0.6.11"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/lodash": "*",
    "@types/node": "^18.11.3",
    "@types/traverse": "^0.6.37",
    "jest": "^29.7.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "endOfLine": "crlf",
    "printWidth": 120,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "none",
    "stylelintIntegration": true
  }
}
